{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "location": {
        "value": "eastus"
      },
      "vnetName": {
        "value": "my-vnet"
      },
      "subnetName": {
        "value": "my-subnet"
      },
      "addressPrefix": {
        "value": "10.0.1.0/24"
      },
      "nsgId": {
        "value": "/subscriptions/<subId>/resourceGroups/<rgName>/providers/Microsoft.Network/networkSecurityGroups/my-nsg"
      }
    }
  }
  